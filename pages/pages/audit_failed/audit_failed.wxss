/* pages/pages/audit_failed/audit_failed.wxss */
.audit_failed{
  color: #7f9fdc;
  font-size: 30rpx;
  width: 690rpx;
  margin: auto;
  background-color: #1a2843;
	border-radius: 30px;
  overflow: hidden;
  padding-bottom: 50rpx;
}

.audit_failed_title{
  width: 630rpx;
  margin:50rpx auto 80rpx auto;
}

.label{
  width: 632rpx;
  height: 50rpx;
  margin:0rpx auto 72rpx auto;
}

.label>view{
  width: 30%;
  height: 50rpx;
  border: 1rpx solid #3bafda;
  background: #1a2843;
  font-size: 24rpx;
  color: #ffffff;
  line-height: 50rpx;
  text-align: center;
  float: left;
  margin-right: 3.33333333%;
}

.label>view:nth-child(3){
  margin-right: 0rpx;
}

.label>.label_view{
  background: #0059a7;
  border-color: #0059a7;
}

.input_box{
  width: 630rpx;
	height: 212rpx;
	border-radius: 10rpx;
	border: solid 1px rgba(165, 193, 247, 0.49);
  margin: auto;
  overflow: hidden;
}

.textarea{
  color: #576c84;
}

.input_box>textarea{
  font-size: 30rpx;
  width: 590rpx;
  height: 150rpx;
  color: #fff;
  margin: 31rpx auto;
}