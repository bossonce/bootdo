/* pages/pages/order_details1/order_details1.wxss */

.data{
  padding-top: 28rpx;
  width: 690rpx;
  margin: auto;
  overflow: hidden;
}

.data>input{
  width: 315rpx;
  height: 60rpx;
	background-color: #1e2d4a;
	border-radius: 10rpx;
  font-size: 26rpx;
  text-align: center;
  line-height: 60rpx;
  color: #Fff;
  display: inline-block;
}

.data>text{
  height: 60rpx;
  width: 60rpx;
  color: #a3b7da;
  line-height: 60rpx;
  text-align: center;  
  display: inline-block;
  font-size: 30rpx;
  vertical-align: top;
}

.title{
  position: relative;
  margin-top: 37rpx; 
}

.for_outbound>view{
  text-align: left;
}

.for_outbound>view>span{
  width: 120rpx;
  text-align-last: justify;
  text-align: justify;
  text-justify: distribute-all-lines;
  margin-right: 30rpx;

}

.for_outbound>.for_outbound_view1>text{
  color: #7f9fdc;
  font-size: 30rpx;
  float: right;
}

.account1{
  padding: 0rpx;
  width: 100%;
}

.account1>span{
  color: #7f9fdc;
  font-size: 30rpx;
  text-align-last: justify;
  text-align: justify;
  text-justify: distribute-all-lines;
  float: left;
}

.for_outbound>.account{
  height: auto;
  border-bottom: 0rpx;
}

.for_outbound_button{
  height: 85rpx;
  width: 100%;
  overflow: hidden;
}

.for_outbound_button>button{
  font-size: 30rpx;
  padding: 17rpx 36rpx;
  background: #37bc9b;
  color: #fff;
  border-radius: 32rpx;
  display: inline-block;
  float: right;
  line-height: 32rpx;
  margin-top: 20rpx;
}

.for_outbound>.for_outbound_fixed{
  width: 660rpx;
  margin-left: -30rpx;
  padding-left: 30rpx;
}

.for_outbound_fixed1{
  overflow: initial;
}

.for_outbound_fixed1>.picture{
  width: 100%;
  padding: 26rpx 0rpx 40rpx 0rpx;
  overflow: hidden;
  height: auto;
}

.for_outbound_fixed1>.picture>view{
  width: 143rpx;
  height: 143rpx;
  float: left;
  margin-right: 20rpx;
}

.for_outbound_fixed1>.picture>view>image{
  width: 100%;
  height: 100%;
}

.for_outbound_fixed1>.picture>view:nth-child(4){
  margin-right: 0rpx;
}

.for_outbound>.for_outbound_view {
  overflow: hidden;
}