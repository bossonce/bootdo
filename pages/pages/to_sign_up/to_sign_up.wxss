/* pages/pages/to_sign_up/to_sign_up.wxss */
.style{
  margin-top: 40rpx;
}

.for_outbound_content{
  width: 630rpx;
  margin: auto;
  overflow: hidden;
}

.for_outbound_content_view>image{
  width: 20rpx;
	height: 26rpx;
  margin-right: 10rpx;
}

.for_outbound_content>view:nth-child(1){
  float: none;
  color: #fff;
}

.for_outbound_content>view:nth-child(2) {
  color: #b1bdf5;
  margin: 25rpx 0rpx 30rpx 0rpx;
}

.for_outbound_content>view:nth-child(3){
	height: 52rpx;
  line-height: 52rpx;
}

.for_outbound_content>view{
  line-height: 30rpx;
}

.for_outbound_content>view>button{
  width: 113rpx;
	height: 52rpx;
	background-color: #37bc9b;
	border-radius: 26rpx;
	border: 0rpx;
  font-size: 28rpx;
  color: #fff;
  padding: 0rpx;
  line-height: 52rpx;
  float: right;
}