  font-size: 32rpx;
  color: #7f9fdc;
  border-bottom: 1rpx solid #293c60;
  margin: auto;
}

.search_lab_view>span {
  line-height: 125rpx;
  margin-left: 19rpx;
}

.search_lab_view>button {
  width: 177rpx;
  height: 63rpx;
  line-height: 63rpx;
  background-color: #37bc9b;
  border-radius: 32rpx;
  float: right;
  font-size: 30rpx;
  color: #fff;
  margin-top: 30rpx;
}

/* //左图片右文字可滑动 */

.recommend_zone {
  width: 690rpx;
  margin: 46rpx auto 0rpx auto;
  overflow: hidden;
}

.recommend_zone_view {
  width: 690rpx;
  height: 220rpx;
  background-color: #1a2843;
  box-shadow: 0rpx 1rpx 27rpx 0rpx rgba(3, 0, 0, 0.1);
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}

.recommend_zone_view>image {
  width: 235rpx;
  height: 160rpx;
  margin: 33rpx 20rpx 0rpx 30rpx;
  float: left;
}

.recommend_zone_view>view {
  height: 160rpx;
  width: 365rpx;
  float: left;
  margin-top: 33rpx;
}

.recommend_zone_view>view>view {
  font-size: 32rpx;
  color: #fff;
}

.recommend_zone_view_label {
  margin-top: 20rpx;
  width: 100%;
  height: 50rpx;
  display: -webkit-box;
  overflow-x: scroll;
  -webkit-overflow-scrolling: touch;
  overflow-y: hidden;
  overflow-scrolling: touch;
}

.recommend_zone_view_label_view {
  font-size: 22rpx;
  color: #fff;
  padding: 5rpx 10rpx;
  background: #37bc9b;
  height: 38rpx;
  margin-right: 10rpx;
  border-radius: 3rpx;
  box-sizing: border-box;
  line-height: 35rpx;
}

.positioning {
  margin-top: 5rpx;
}

.positioning>span {
  font-size: 22rpx;
  color: #6288cc;
  margin-right: 8rpx;
}

.positioning>image {
  width: 18rpx;
  height: 23rpx;
  vertical-align: middle;
  margin-right: 8rpx;
}

/* //点击下拉单对单 */

.account {
  background-color: #1a2843;
  box-shadow: 0px 1rpx 27rpx 0rpx rgba(3, 0, 0, 0.1);
  border-radius: 16rpx;
  font-size: 30rpx;
  color: #7f9fdc;
  overflow: hidden;
  margin-bottom: 30rpx;
}

.enter_an_item_in_an_account {
  width: 28rpx;
  height: 25rpx;
}

.white_down_arrow {
  width: 25rpx;
  height: 14rpx;
  float: right;
  margin-top: 38rpx;
  transform: rotate(180deg);
}

.account_an {
  height: 160rpx;
  width: 650rpx;
  border-bottom: 1rpx solid #293c60;
  margin: auto;
}

.account_an>view {
  float: left;
  width: 60%;
  color: #fff;
  margin-top: 40rpx;
  text-align: left;
  /* margin-left: 18rpx; */
}

.account_an>view>view {
  margin-top: 12rpx;
}

.account_an>view>view>view {
  display: inline-block;
  margin-right: 29rpx;
  font-size: 26rpx;
  color: #6288cc;
}

.account_an>button {
  font-size: 28rpx;
  color: #fefefe;
  padding: 19rpx 28rpx;
  display: inline-block;
  background: #37bc9b;
  line-height: 28rpx;
  float: right;
  margin-top: 43rpx;
  margin-right: 30rpx;
}

/* 下拉动画 */

/* @keyframes Css3 新标签，循环动画 */

.temp {
  overflow: hidden;
}

@keyframes slidown {
  from {
    transform: translateY(-100%);
  }

  to {
    transform: translateY(0%);
  }
}

.slidown {
  /* display: block; */
  animation: slidown 0.2s ease-in both;
}

/* 上拉动画 */

@keyframes slidup {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(-100%);
  }
}

.slidup {
  /* display: block; */
  animation: slidup 0.2s ease-in both;
  height: 0px;
}

.account1 {
  position: relative;
