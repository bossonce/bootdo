/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  height: 100%;
  width: 100%;
  background: #111a2b;
}

/* //最外层view */

.style {
  margin-bottom: 120rpx;
  overflow: hidden;
}

.stay {
  width: 690rpx;
  margin: auto;
  border-radius: 15rpx;
  background-color: #1a2843;
  padding-bottom: 57rpx;
}

/* //底部按钮 */

.submit_audit {
  width: 750rpx;
  height: 98rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 98rpx;
  background: linear-gradient(94deg, rgba(33, 103, 239, 1), rgba(55, 144, 239, 1));
  box-shadow: 0px -1px 0px 0px #34528a;
  border-radius: 0rpx;
  padding: 0rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 50;
}

/* //底部双按钮 */

.but {
  width: 100%;
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
  color: #fff;
  font-size: 30rpx;
}

.but>view {
  width: 50%;
  float: left;
}

.but>view:nth-child(1) {
  background: rgba(42, 126, 219, 0.35);
}

.but>view:nth-child(2) {
  background: #0059a7;
}

.but>view>image {
  width: 29rpx;
  height: 29rpx;
  margin-right: 10rpx;
  vertical-align: middle;
  margin-top: -7rpx;
}

/* //搜索 */

.title {
  height: 132rpx;
  background: #0d1321;
  width: 690rpx;
  padding: 0rpx 30rpx;
  overflow: hidden;
}

.title_search {
  width: 586rpx;
  height: 60rpx;
  background-color: #1e2d4a;
  border-radius: 10rpx;
  text-align: center;
  line-height: 60rpx;
  /* margin-top: 40rpx; *//* vertical-align: middle; *//* display: inline-block; */
}

.title_search>input {
  color: #fff;
  font-size: 26rpx;
  width: 295rpx;
  text-align: left;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: middle;
}

.title_search_placeholder {
  color: #a3b7da;
}

.title_search>image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}

.search {
  float: right;
  width: 94rpx;
  height: 60rpx;
  background: linear-gradient(94deg, rgba(33, 103, 239, 1), rgba(55, 144, 239, 1));
  background-blend-mode: normal, normal;
  border-radius: 10rpx;
  color: #fff;
  font-size: 26rpx;
  padding: 0px;
  margin-top: 40rpx;
}

/* //有地址定位的搜索 */

.title {
  height: auto;
  overflow: hidden;
  margin-bottom: 30rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  background: #111a2b;
  z-index: 100;
}

.title1 {
  height: 60rpx;
  font-size: 28rpx;
  color: #fff;
  width: 150rpx;
  float: left;
  line-height: 60rpx;
}

.title1>.title1_img1 {
  width: 26rpx;
  height: 30rpx;
  vertical-align: sub;
  margin-right: 10rpx;
  margin-top: 18rpx;
}

.title1>.title1_img2 {
  width: 17rpx;
  height: 8rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.title2 {
  width: 436rpx;
  height: 60rpx;
  background-color: #1e2d4a;
  border-radius: 10rpx;
  float: left;
  line-height: 60rpx;
}

.title2>image {
  width: 24rpx;
  height: 24rpx;
  margin-left: 20rpx;
  float: left;
  margin-top: 20rpx;
}

.title2>input {
  color: #fff;
  font-size: 26rpx;
  width: 305rpx;
  float: left;
  margin-left: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.title2_placeholder {
  color: #a3b7da;
}

.title>.search {
